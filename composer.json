{
    "name": "omobude/dhl-symfony-bundle",
    "description": "Modern Symfony 7+ bundle for DHL API integration. Create shipment labels and download DHL shipment labels with ease.",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Omobude Kelly",
            "email": "k.omobude2019@gmail.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "psr/log": "^3.0",
        "symfony/cache": "^7.0",
        "symfony/framework-bundle": "^7.0",
        "symfony/http-client": "^7.0",
        "symfony/dependency-injection": "^7.0",
        "symfony/config": "^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "symfony/phpunit-bridge": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "Omobude\\DhlBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Omobude\\DhlBundle\\Tests\\": "tests/"
        }
    },
    "keywords": [
        "dhl",
        "symfony",
        "bundle",
        "shipping",
        "logistics",
        "api",
        "parcel",
        "tracking",
        "label",
        "oauth"
    ]
}
